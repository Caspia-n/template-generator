{
  "name": "notion-template-generator",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "electron .",
    "electron:dev": "concurrently \"cross-env BROWSER=none next dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron:build": "next build && electron-builder",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write ."
  },
  "build": {
    "appId": "com.notion-template-generator",
    "productName": "Notion Template Generator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^0.1.0",
    "@heroui/react": "^3.0.0-beta.3",
    "@hookform/resolvers": "^5.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@notionhq/client": "^2.2.15",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "framer-motion": "^11.15.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.469.0",
    "nanoid": "^5.0.9",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "node-llama-cpp": "^3.0.0",
    "notion-types": "^7.4.4",
    "notion-utils": "^7.4.4",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "react-notion-x": "^7.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^25.2.0",
    "eslint": "^9.15.0",
    "eslint-config-next": "^16.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
